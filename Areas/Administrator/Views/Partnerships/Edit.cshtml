@model CarDoze.Areas.Administrator.ViewModels.PartnershipViewModel

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Areas/Administrator/Views/_Layout2.cshtml";
}

<h2>Edit</h2>

<h4>Partnership</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit" method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="PartnershipID" />
            <div class="form-group">
                <label asp-for="TypeShipp" class="control-label"></label>
                <input asp-for="TypeShipp" class="form-control" />
                <span asp-validation-for="TypeShipp" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Logo" class="control-label"></label>
                <input asp-for="Logo" class="form-control" />
                <span asp-validation-for="Logo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Description" class="control-label"></label>
                <input asp-for="Description" class="form-control" />
                <span asp-validation-for="Description" class="text-danger"></span>
            </div>
      
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a href="/Partnership/Index">Back to List</a>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
